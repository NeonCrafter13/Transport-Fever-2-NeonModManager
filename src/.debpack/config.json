{
    "section": "utils",
    "priority": "optional",
    "maintainer": {
        "name": "NeonCrafter13",
        "email": "neon.crafter.xd@gmail.com"
    },
    "depends": [
        "python3",
        "python3-pyqt5",
        "python3-numpy",
        "p7zip"
